{
  "name": "@nader-eloshaiker/mat-datetimepicker",
  "version": "4.0.2",
  "license": "MIT",
  "private": false,
  "description": "Angular Material DateTime Picker",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nader-eloshaiker/mat-datetimepicker.git"
  },
  "keywords": [
    "Angular8",
    "Angular",
    "DateTime",
    "Picker",
    "Moment"
  ],
  "author": "Nader Eloshaiker",
  "bugs": {
    "url": "https://github.com/nader-eloshaiker/mat-datetimepicker/issues"
  },
  "homepage": "https://github.com/nader-eloshaiker/mat-datetimepicker#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "npm run build:lib && ng build",
    "build:lib": "npm run build:lib/core && npm run build:lib/moment",
    "build:lib/core": "ng build core && copy-files-from-to --config projects/core/copy-files.json",
    "build:lib/moment": "ng build moment && copy-files-from-to --config projects/moment/copy-files.json",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/material": "~8.2.3",
    "@angular/material-moment-adapter": "~8.2.3",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "copy-files-from-to": "1.4.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "moment": "2.23.0",
    "rxjs": "~6.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.13",
    "@angular-devkit/build-ng-packagr": "~0.803.13",
    "@angular/cli": "~8.3.13",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.18",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.32",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.4.1",
    "tsickle": "^0.37.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
