{
  "name": "@nader-eloshaiker/mat-datetimepicker",
  "version": "5.1.1",
  "license": "MIT",
  "private": false,
  "homepage": "https://nader-eloshaiker.github.io/mat-datetimepicker/",
  "description": "Angular Material DateTime Picker",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nader-eloshaiker/mat-datetimepicker.git"
  },
  "keywords": [
    "Angular9",
    "Angular8",
    "Angular",
    "Date",
    "Time",
    "DateTime",
    "Picker",
    "Moment"
  ],
  "author": "Nader Eloshaiker",
  "bugs": {
    "url": "https://github.com/nader-eloshaiker/mat-datetimepicker/issues"
  },
  "resolutions": {
    "minimist": "1.2.3"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "npm run build:lib && ng build --prod",
    "build:lib": "npm run build:lib/core && npm run build:lib/moment",
    "build:lib/core": "ng build core && copy-files-from-to --config projects/core/copy-files.json",
    "build:lib/moment": "ng build moment && copy-files-from-to --config projects/moment/copy-files.json",
    "build:github": "npm run build:lib && ng build --prod --base-href=https://nader-eloshaiker.github.io/mat-datetimepicker/",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/material-moment-adapter": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "core-js": "^2.5.4",
    "moment": "^2.24.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular-devkit/build-ng-packagr": "~0.900.7",
    "@angular/cli": "^9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "copy-files-from-to": "1.4.0",
    "jasmine-core": "~3.5.0",
    "jasmine-reporters": "^2.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.32",
    "ng-packagr": "^10.1.1",
    "protractor": "~5.4.4",
    "rollup": "^1.20.0",
    "ts-node": "~8.4.1",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5"
  }
}
