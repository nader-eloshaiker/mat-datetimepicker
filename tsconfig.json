{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@nader-eloshaiker/mat-datetimepicker": [
        "dist/core"
      ],
      "@nader-eloshaiker/mat-datetimepicker/*": [
        "dist/core/*"
      ],
      "@nader-eloshaiker/mat-datetimepicker-moment": [
        "dist/moment"
      ],
      "@nader-eloshaiker/mat-datetimepicker-moment/*": [
        "dist/moment/*"
      ]
    }
  }
}
